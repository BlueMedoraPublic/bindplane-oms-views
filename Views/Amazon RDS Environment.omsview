{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Amazon RDS Environment",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Amazon RDS Environment')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Amazon RDS Environment",
                        "Name": "Amazon RDS Environment",
                        "Author": "service.azure@bluemedora.com",
                        "Source": "Local",
                        "Version": 2,
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "RDS Overview",
                                        "NewGroup": false,
                                        "Color": "#0072c6"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Amazon RDS",
                                        "Link": {
                                            "Label": "More info",
                                            "Url": "https://docs.bindplane.bluemedora.com/docs/rds"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "Overview",
                                            "Content": "Amazon RDS is a cloud hosted service for creating and managing relational databases."
                                        },
                                        {
                                            "Title": "Key Performance Indicators",
                                            "Content": "In Amazon RDS, it's important to monitor the number of connections to a database instance. This is an early indicator of instance load.\n\nLikewise, it's also important to monitor the CPU, IOPS, and throughput of an instance to troublshoot potential bottlenecks."
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "TwoNumberTileListBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "RDS Snapshots",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Tile": {
                                        "Legend": "Instances",
                                        "Query": "amazon_rds_instance_CL | distinct amazon_rds_instance_id_g, amazon_rds_instance_id_s | count"
                                    },
                                    "SecondTile": {
                                        "Legend": "Snapshots",
                                        "Query": "amazon_rds_snapshot_CL | distinct amazon_rds_snapshot_id_g, amazon_rds_snapshot_id_s | count"
                                    },
                                    "List": {
                                        "Query": "amazon_rds_snapshot_CL | distinct amazon_rds_snapshot_id_g, amazon_rds_snapshot_id_s, db_instance_identifier_s | summarize count() by db_instance_identifier_s | order by count_ desc",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Instance",
                                            "Value": "Snapshots"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    },
                                    "Blade": {
                                        "NavigationSelect": {}
                                    }
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "RDS Connections",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Connections",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | extend Connections = database_connections_d, Type = engine_s, Name = amazon_rds_instance_name_s | summarize round(avg(Connections),0) by Name, Type",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": "Connections"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | extend Connections = database_connections_d, Type = engine_s, Name = amazon_rds_instance_name_s | summarize round(avg(Connections),0) by Name, Type",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Last Sample",
                                        "ColumnsTitle": {
                                            "Name": "Instance",
                                            "Value": "Connections"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "RDS CPU",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "CPU Utilization",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | summarize avg(cpu_utilization_d) by amazon_rds_instance_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Percentage",
                                                        "baseUnit": "ZeroToOne",
                                                        "displayUnit": "ZeroToOne"
                                                    },
                                                    "customLabel": "Utilization"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "CPU Credit Usage",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | summarize avg(cpu_credit_usage_d) by amazon_rds_instance_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Credits"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "CPU Credit Balance",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | summarize avg(cpu_credit_balance_d) by amazon_rds_instance_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Credits"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "RDS Operations",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Total Latency",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | extend Latency = (read_latency_d + write_latency_d)*1000, Instance = amazon_rds_instance_name_s | summarize avg(Latency) by Instance",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "TimeRange",
                                                        "baseUnit": "Milliseconds",
                                                        "displayUnit": "AUTO"
                                                    },
                                                    "customLabel": "Latency"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Total IOPS",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | extend IOPS = read_iops_d + write_iops_d, Instance = amazon_rds_instance_name_s | summarize avg(IOPS) by Instance",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "IOPS"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Total Throughput",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_rds_instance_CL | where ${Instance} and ${Cluster} | extend Throughput = read_throughput_d + write_throughput_d, Instance = amazon_rds_instance_name_s | summarize avg(Throughput) by Instance",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Bits",
                                                        "baseUnit": "Bytes",
                                                        "displayUnit": "AUTO"
                                                    },
                                                    "customLabel": "Throughput"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Additional Info",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "amazon_rds_instance_CL  | join kind= inner (    amazon_rds_instance_CL    | summarize TimeGenerated=max(TimeGenerated) by amazon_rds_instance_name_s ) on TimeGenerated, amazon_rds_instance_name_s | project amazon_rds_instance_name_s, storage_encrypted_d, storage_type_s, allocated_storage_d, free_storage_space_d",
                                            "displayName": "RDS Storage"
                                        },
                                        {
                                            "query": "amazon_rds_instance_CL | distinct  amazon_rds_instance_name_s, engine_s, engine_version_s, endpoint_address_s, endpoint_port_d",
                                            "displayName": "RDS Database Configuration"
                                        },
                                        {
                                            "query": "amazon_rds_instance_CL  | distinct amazon_rds_instance_name_s, availability_zone_s, secondary_availability_zone_s, multi_az_d",
                                            "displayName": "RDS Availability Zone"
                                        },
                                        {
                                            "query": "amazon_rds_instance_CL  | distinct subnet_group_name_s, amazon_rds_instance_name_s, subnet_group_vpc_id_s, subnet_group_description_s, subnet_group_status_s ",
                                            "displayName": "RDS Subnet Info"
                                        },
                                        {
                                            "query": "amazon_rds_snapshot_CL | distinct amazon_rds_snapshot_name_s, db_instance_identifier_s, snapshot_create_time_s, snapshot_type_s, provisioned_iops_d",
                                            "displayName": "RDS Snapshots"
                                        },
                                        {
                                            "query": "amazon_rds_tag_CL | distinct amazon_rds_rds_instance_name_s, amazon_rds_tag_name_s, value_s | order by amazon_rds_rds_instance_name_s desc",
                                            "displayName": "RDS Tags"
                                        }
                                    ]
                                }
                            }
                        ],
                        "Filters": [
                            {
                                "FieldName": "amazon_rds_cluster_name_s",
                                "QueryForValues": "amazon_rds_cluster_CL | distinct amazon_rds_cluster_name_s",
                                "Title": "Cluster"
                            },
                            {
                                "FieldName": "amazon_rds_instance_name_s",
                                "QueryForValues": "amazon_rds_instance_CL | distinct amazon_rds_instance_name_s",
                                "Title": "Instance"
                            }
                        ],
                        "OverviewTile": {
                            "Id": "LineChartCalloutBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 2,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "amazon_rds_instance_CL | extend Connections = database_connections_d, Type = engine_s, Name = amazon_rds_instance_name_s | summarize round(avg(Connections),0) by Name, Type",
                                    "Callout": {
                                        "Title": "Average Connections",
                                        "Series": "",
                                        "Operation": "Average"
                                    },
                                    "yAxis": {
                                        "isLogarithmic": false,
                                        "units": {
                                            "baseUnitType": "",
                                            "baseUnit": "",
                                            "displayUnit": ""
                                        },
                                        "customLabel": "Connections"
                                    }
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": false,
                                        "Query": "search * | limit 1 | project TimeGenerated",
                                        "Message": ""
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}