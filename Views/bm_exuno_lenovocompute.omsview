                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "lenovo_compute_chassis_CL | project ['System Health State'] = columnifexists('system_health_state_s', 'NULL_VALUE'), TimeGenerated, Name=lenovo_compute_chassis_name_s  | where {selected item}  | summarize arg_max(TimeGenerated, *) by Name | extend Healthy = case(coalesce(['System Health State'], 'NULL_VALUE') != 'NULL_VALUE', (['System Health State'] == 'Normal' or coalesce(['System Health State'], 'NULL_VALUE') == 'NULL_VALUE'), bool(null))",
                                        "NavigationSelect": {
                                            "NavigationQuery": "lenovo_compute_chassis_CL | project ['System Health State'] = columnifexists('system_health_state_s', 'NULL_VALUE'), TimeGenerated, Name=lenovo_compute_chassis_name_s  | where {selected item}  | summarize arg_max(TimeGenerated, *) by Name | extend Healthy = case(coalesce(['System Health State'], 'NULL_VALUE') != 'NULL_VALUE', (['System Health State'] == 'Normal' or coalesce(['System Health State'], 'NULL_VALUE') == 'NULL_VALUE'), bool(null))"
                                        }
                                    }
                                }
                            },

