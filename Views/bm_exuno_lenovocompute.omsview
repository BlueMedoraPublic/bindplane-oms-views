,
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Additional Info",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [

                                        {
                                            "query": "lenovo_compute_chassis_CL | summarize([\"Last Update Time\"], ['System Health State'])=arg_max(TimeGenerated,columnifexists('system_health_state_s', '')) by Name=lenovo_compute_chassis_name_s",
                                            "displayName": "Chassis Status"
                                        }
                                        ,{
                                            "query": "lenovo_compute_chassis_CL | summarize([\"Last Update Time\"], ['Bays Supported'], ['Fans Supported'], ['Machine Type'], ['Management Modules Supported'], ['Power Modules Supported'], ['Serial Number'], ['Switch Modules Supported'])=arg_max(TimeGenerated,columnifexists('bays_supported_d', ''),columnifexists('fans_supported_d', ''),columnifexists('machine_type_s', ''),columnifexists('management_modules_supported_d', ''),columnifexists('power_modules_supported_d', ''),columnifexists('serial_number_s', ''),columnifexists('switch_modules_supported_d', '')) by Name=lenovo_compute_chassis_name_s",
                                            "displayName": "Chassis Properties"
                                        }
                                        ,{
                                            "query": "lenovo_compute_compute_server_CL | summarize([\"Last Update Time\"], ['Access State'], ['Health Status'], ['Power Status'])=arg_max(TimeGenerated,columnifexists('access_state_s', ''),columnifexists('health_status_s', ''),columnifexists('power_status_s', '')) by Name=lenovo_compute_compute_server_name_s",
                                            "displayName": "Server Status"
                                        }
                                        ,{
                                            "query": "lenovo_compute_compute_server_CL | summarize([\"Last Update Time\"], ['MAC Addresses'], ['Model'], ['Product Name'], ['Serial Number'], ['Type'])=arg_max(TimeGenerated,columnifexists('mac_addresses_s', ''),columnifexists('model_s', ''),columnifexists('product_name_s', ''),columnifexists('serial_number_s', ''),columnifexists('type_s', '')) by Name=lenovo_compute_compute_server_name_s",
                                            "displayName": "Server Properties"
                                        }
                                        ,{
                                            "query": "lenovo_compute_flex_switch_CL | summarize([\"Last Update Time\"], ['Access State'], ['Health State'])=arg_max(TimeGenerated,columnifexists('access_state_s', ''),columnifexists('health_state_s', '')) by Name=lenovo_compute_flex_switch_name_s",
                                            "displayName": "Flex Switch Status"
                                        }
                                        ,{
                                            "query": "lenovo_compute_flex_switch_CL | summarize([\"Last Update Time\"], ['Maximum Processes'], ['Processes'])=arg_max(TimeGenerated,columnifexists('maximum_processes_d', ''),columnifexists('processes_d', '')) by Name=lenovo_compute_flex_switch_name_s",
                                            "displayName": "Flex Switch Details"
                                        }
                                        ,{
                                            "query": "lenovo_compute_flex_switch_CL | summarize([\"Last Update Time\"], ['Description'], ['Model'], ['Serial Number'], ['Type'])=arg_max(TimeGenerated,columnifexists('description_s', ''),columnifexists('model_s', ''),columnifexists('serial_number_s', ''),columnifexists('type_s', '')) by Name=lenovo_compute_flex_switch_name_s",
                                            "displayName": "Flex Switch Properties"
                                        }
                                        ,{
                                            "query": "lenovo_compute_processor_CL | summarize([\"Last Update Time\"], ['Speed'])=arg_max(TimeGenerated,columnifexists('speed_d', '')) by Name=lenovo_compute_processor_name_s",
                                            "displayName": "Processor Performance"
                                        }
                                        ,{
                                            "query": "lenovo_compute_processor_CL | summarize([\"Last Update Time\"], ['Cores'], ['Family'])=arg_max(TimeGenerated,columnifexists('cores_s', ''),columnifexists('family_s', '')) by Name=lenovo_compute_processor_name_s",
                                            "displayName": "Processor Properties"
                                        }
                                    ]
                                }
                            }
                        ],
                        "Filters": [{
                            "FieldName": "machine_type_s",
                            "QueryForValues": "lenovo_compute_chassis_CL | distinct machine_type_s",
                            "Title": "Machine Type"
                          },{
                            "FieldName": "system_health_state_s",
                            "QueryForValues": "lenovo_compute_chassis_CL | distinct system_health_state_s",
                            "Title": "System Health State"
                          },{
                            "FieldName": "lenovo_compute_chassis_name_s",
                            "QueryForValues": "union lenovo_compute_chassis_CL, lenovo_compute_compute_server_CL, lenovo_compute_flex_switch_CL | distinct lenovo_compute_chassis_name_s",
                            "Title": "Chassis"
                          },{
                            "FieldName": "model_s",
                            "QueryForValues": "union lenovo_compute_compute_server_CL, lenovo_compute_flex_switch_CL | distinct model_s",
                            "Title": "Model"
                          },{
                            "FieldName": "type_s",
                            "QueryForValues": "union lenovo_compute_compute_server_CL, lenovo_compute_flex_switch_CL | distinct type_s",
                            "Title": "Type"
                          },{
                            "FieldName": "lenovo_compute_compute_server_name_s",
                            "QueryForValues": "union lenovo_compute_compute_server_CL, lenovo_compute_processor_CL | distinct lenovo_compute_compute_server_name_s",
                            "Title": "Server"
                          },{
                            "FieldName": "lenovo_compute_flex_switch_name_s",
                            "QueryForValues": "lenovo_compute_flex_switch_CL | distinct lenovo_compute_flex_switch_name_s",
                            "Title": "Flex Switch"
                          },{
                            "FieldName": "family_s",
