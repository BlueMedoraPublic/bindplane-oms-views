                            "Title": "Family"
                          },{
                            "FieldName": "lenovo_compute_processor_name_s",
                            "QueryForValues": "lenovo_compute_processor_CL | distinct lenovo_compute_processor_name_s",
                            "Title": "Processor"
                          }],
                        "OverviewTile": {
                            "Id": "SingleQueryDonutBuilderTileV1",
                            "Type": "OverviewTile",
                            "Version": 2,
                            "Configuration": {
                                "Donut": {
                                    "Query": "lenovo_compute_chassis_CL | project ['System Health State'] = columnifexists('system_health_state_s', 'NULL_VALUE'), TimeGenerated, Name=lenovo_compute_chassis_name_s  | extend Healthy = case(coalesce(['System Health State'], 'NULL_VALUE') != 'NULL_VALUE', (['System Health State'] == 'Normal' or coalesce(['System Health State'], 'NULL_VALUE') == 'NULL_VALUE'), bool(null)) | where isnotnull(Healthy) | summarize HealthScore = avg(Healthy) * 100 by Name | summarize count() by case(HealthScore < 50, 'Critical', HealthScore < 70, 'Warning', isnull(HealthScore) or isnan(HealthScore), 'Unknown', 'Healthy')",
                                    "CenterLegend": {
                                        "Text": "Chassis",
                                        "Operation": "Sum",
                                        "ArcsToSelect": []
                                    },
