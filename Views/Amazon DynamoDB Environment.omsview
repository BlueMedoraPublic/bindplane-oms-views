{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Amazon DynamoDB Environment",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Amazon DynamoDB Environment')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Amazon DynamoDB Environment",
                        "Name": "Amazon DynamoDB Environment",
                        "Author": "service.azure@bluemedora.com",
                        "Source": "Local",
                        "Version": 2,
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "DynamoDB Overview",
                                        "NewGroup": false,
                                        "Color": "#0072c6"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Amazon DynamoDB",
                                        "Link": {
                                            "Label": "More Info",
                                            "Url": "https://docs.bindplane.bluemedora.com/docs/dynamodb"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "Overview",
                                            "Content": "Amazon DynamoDB is a fully managed cloud database and supports both document and key-value store models.\n"
                                        },
                                        {
                                            "Title": "Key Performance Indicators",
                                            "Content": "With DynamoDB, it is important to monitor capacity units and throttle events.\n\nEvery DynamoDB table is provisioned with read and write capacity units. When consumption surpasses the provisioned amount, throttling of the service will occur."
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "DynamoDB Table Reads",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Provisioned Read Capacity",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(provisioned_read_capacity_units_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Count",
                                                        "baseUnit": "Ones",
                                                        "displayUnit": "Ones"
                                                    },
                                                    "customLabel": "Units"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Consumed Read Capacity",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(consumed_read_capacity_units_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Count",
                                                        "baseUnit": "Ones",
                                                        "displayUnit": "AUTO"
                                                    },
                                                    "customLabel": "Units"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Read Throttles",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(read_throttle_events_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Count",
                                                        "baseUnit": "Ones",
                                                        "displayUnit": "Ones"
                                                    },
                                                    "customLabel": "Throttles"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "DynamoDB Table Writes",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Provisioned Write Capacity",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(provisioned_write_capacity_units_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Units"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Consumed Write Capacity",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(consumed_write_capacity_units_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Units"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Write Throttles",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_dynamodb_table_CL | where ${Table} | summarize sum(write_throttle_events_d) by amazon_dynamodb_table_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Count",
                                                        "baseUnit": "Ones",
                                                        "displayUnit": "Ones"
                                                    },
                                                    "customLabel": "Throttles"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "DynamoDB Item Count",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Items",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_dynamodb_global_secondary_index_CL | where ${Table} | summarize avg(item_count_d) by amazon_dynamodb_table_name_s",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": "Items"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_dynamodb_global_secondary_index_CL  | where isnotempty(amazon_dynamodb_table_name_s) | where ${Table} | summarize avg(item_count_d) by amazon_dynamodb_table_name_s | order by avg_item_count_d desc",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Table",
                                            "Value": "Items"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Additional Info",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "amazon_dynamodb_table_CL | join kind= inner(     amazon_dynamodb_table_CL     | summarize TimeGenerated=max(TimeGenerated) by amazon_dynamodb_table_name_s ) on TimeGenerated  | project amazon_dynamodb_table_name_s, region_s, status_s",
                                            "displayName": "Table Status"
                                        },
                                        {
                                            "query": "amazon_dynamodb_table_CL | summarize avg(successful_request_latency_d) by amazon_dynamodb_table_name_s",
                                            "displayName": "Table Request Latency"
                                        },
                                        {
                                            "query": "amazon_dynamodb_table_CL | summarize sum(user_errors_d), sum(system_errors_d) by amazon_dynamodb_table_name_s",
                                            "displayName": "Table Errors"
                                        },
                                        {
                                            "query": "amazon_dynamodb_key_schema_CL | distinct amazon_dynamodb_key_schema_name_s, index_name_s, table_name_s, region_s, role_s",
                                            "displayName": "Key Schema"
                                        },
                                        {
                                            "query": "amazon_dynamodb_global_secondary_index_CL | join kind= inner( amazon_dynamodb_global_secondary_index_CL | summarize TimeGenerated=max(TimeGenerated) by amazon_dynamodb_global_secondary_index_name_s ) on TimeGenerated, amazon_dynamodb_global_secondary_index_name_s | project amazon_dynamodb_global_secondary_index_name_s, amazon_dynamodb_table_name_s, item_count_d, size_d, status_s",
                                            "displayName": "Index Count"
                                        }
                                    ]
                                }
                            }
                        ],
                        "Filters": [
                            {
                                "FieldName": "amazon_dynamodb_table_name_s",
                                "QueryForValues": "amazon_dynamodb_table_CL | distinct amazon_dynamodb_table_name_s",
                                "Title": "Table"
                            }
                        ],
                        "OverviewTile": {
                            "Id": "LineChartCalloutBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 2,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "amazon_dynamodb_table_CL | summarize sum(read_throttle_events_d + write_throttle_events_d) by amazon_dynamodb_table_name_s",
                                    "Callout": {
                                        "Title": "Total Throttles",
                                        "Series": "",
                                        "Operation": "Sum"
                                    },
                                    "yAxis": {
                                        "isLogarithmic": false,
                                        "units": {
                                            "baseUnitType": "",
                                            "baseUnit": "",
                                            "displayUnit": ""
                                        },
                                        "customLabel": "Throttles"
                                    }
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": false,
                                        "Query": "search * | limit 1 | project TimeGenerated",
                                        "Message": ""
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}