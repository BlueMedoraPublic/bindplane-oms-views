                                        }
                                        ,{
                                            "query": "ibm_softlayer_hardware_CL | summarize([\"Last Update Time\"], ['Average Daily Usage'], ['Private In Throughput'], ['Private Out Throughput'], ['Public In Throughput'], ['Public Out Throughput'])=arg_max(TimeGenerated,columnifexists('average_daily_usage_d', ''),columnifexists('private_in_throughput_d', ''),columnifexists('private_out_throughput_d', ''),columnifexists('public_in_throughput_d', ''),columnifexists('public_out_throughput_d', '')) by Name=ibm_softlayer_hardware_name_s",
