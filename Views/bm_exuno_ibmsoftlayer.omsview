                                            "query": "ibm_softlayer_event_notification_CL | summarize([\"Last Update Time\"], ['End Date'], ['Notification Occurrence Event Type'], ['Start Date'], ['Subject'], ['Summary First Line'])=arg_max(TimeGenerated,columnifexists('end_date_s', ''),columnifexists('notification_occurrence_event_type_s', ''),columnifexists('start_date_s', ''),columnifexists('subject_s', ''),columnifexists('summary_first_line_s', '')) by Name=ibm_softlayer_event_notification_name_s",
