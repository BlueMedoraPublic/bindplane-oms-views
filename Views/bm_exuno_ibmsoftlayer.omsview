                                            "query": "ibm_softlayer_hardware_CL | summarize([\"Last Update Time\"], ['Average Memory Usage'])=arg_max(TimeGenerated,columnifexists('average_memory_usage_d', '')) by Name=ibm_softlayer_hardware_name_s",
                                            "displayName": "Hardware Memory"
                                        }
                                        ,{
                                            "query": "ibm_softlayer_hardware_CL | summarize([\"Last Update Time\"], ['CPU Average'])=arg_max(TimeGenerated,columnifexists('cpu_average_d', '')) by Name=ibm_softlayer_hardware_name_s",
                                            "displayName": "Hardware CPU"
                                        }
                                        ,{
                                            "query": "ibm_softlayer_hardware_CL | summarize([\"Last Update Time\"], ['Bandwidth Allocation'], ['Datacenter'], ['Hostname'], ['Is Hourly Billed'], ['Memory Capacity'], ['Primary Backend IP Address'], ['Primary Public IP Address'])=arg_max(TimeGenerated,columnifexists('bandwidth_allocation_d', ''),columnifexists('datacenter_s', ''),columnifexists('hostname_s', ''),columnifexists('is_hourly_billed_d', ''),columnifexists('memory_capacity_d', ''),columnifexists('primary_backend_ip_address_s', ''),columnifexists('primary_public_ip_address_s', '')) by Name=ibm_softlayer_hardware_name_s",
