{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Amazon Kinesis Environment",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Amazon Kinesis Environment')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Amazon Kinesis Environment",
                        "Name": "Amazon Kinesis Environment",
                        "Author": "service.azure@bluemedora.com",
                        "Source": "Local",
                        "Version": 2,
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "Kinesis Overview",
                                        "NewGroup": false,
                                        "Color": "#dd5900"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Amazon Kinesis",
                                        "Link": {
                                            "Label": "More info",
                                            "Url": "https://docs.bindplane.bluemedora.com/docs/kinesis"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "Overview",
                                            "Content": "Amazon Kinesis is a fully managed, cloud hosted streaming service, capable of processing realtime data."
                                        },
                                        {
                                            "Title": "Key Performance Indicators",
                                            "Content": "In Amazon Kinesis, it is important to monitor the amount of data being written and read from a stream. \n\nEach stream is given a set capacity based on its level of sharding. When this capacity is exceeded, operations may be throttled, resulting in an interuption of service."
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Kinesis Reads",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Records Read",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(get_records_records_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Records"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Data Read",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(get_records_data_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Bits",
                                                        "baseUnit": "Bytes",
                                                        "displayUnit": "Bytes"
                                                    },
                                                    "customLabel": "Data"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Read Latency",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize avg(get_records_latency_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "TimeRange",
                                                        "baseUnit": "Milliseconds",
                                                        "displayUnit": "Milliseconds"
                                                    },
                                                    "customLabel": "Latency"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Kinesis Read Throttles",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Read Throughput Exceeded",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(iif(isnull(read_provisioned_throughput_exceeded_d),0.0,read_provisioned_throughput_exceeded_d )) by amazon_kinesis_stream_name_s | order by sum_ desc",
                                        "Callout": {
                                            "Title": "Records Throttled",
                                            "Series": "",
                                            "Operation": "Sum"
                                        },
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": "Records"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(iif(isnull(read_provisioned_throughput_exceeded_d),0.0,read_provisioned_throughput_exceeded_d )) by amazon_kinesis_stream_name_s | order by sum_ desc",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Stream",
                                            "Value": "Records"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Kinesis Writes",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Records Written",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(incoming_records_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Records"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Data Written",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(incoming_data_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "Bits",
                                                        "baseUnit": "Bytes",
                                                        "displayUnit": "Bytes"
                                                    },
                                                    "customLabel": "Data"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Write Latency",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize avg(put_record_latency_d) by amazon_kinesis_stream_name_s",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "TimeRange",
                                                        "baseUnit": "Milliseconds",
                                                        "displayUnit": "AUTO"
                                                    },
                                                    "customLabel": "Latency"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Kinesis Write Throttles",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Write Throughput Exceeded",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_kinesis_stream_CL | where ${Stream} | summarize sum(iif(isnull(write_provisioned_throughput_exceeded_d),0.0,write_provisioned_throughput_exceeded_d  )) by amazon_kinesis_stream_name_s  | order by sum_ desc",
                                        "Callout": {
                                            "Title": "Records Throttled",
                                            "Series": "",
                                            "Operation": "Sum"
                                        },
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": "Records"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_kinesis_stream_CL  | where ${Stream} | summarize sum(iif(isnull(write_provisioned_throughput_exceeded_d),0.0,write_provisioned_throughput_exceeded_d  )) by amazon_kinesis_stream_name_s  | order by sum_ desc",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Stream",
                                            "Value": "Records"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Additional Info",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "amazon_kinesis_stream_CL  | summarize avg(get_records_iterator_age_d) by amazon_kinesis_stream_name_s",
                                            "displayName": "Kinesis Iterator Age"
                                        },
                                        {
                                            "query": "amazon_kinesis_stream_CL  | summarize avg(get_records_success_d) by amazon_kinesis_stream_name_s",
                                            "displayName": "Kinesis Read Success Rate"
                                        },
                                        {
                                            "query": "amazon_kinesis_stream_CL  | summarize avg(put_record_success_d) by amazon_kinesis_stream_name_s",
                                            "displayName": "Kinesis Write Success Rate"
                                        },
                                        {
                                            "query": "amazon_kinesis_stream_CL  | join kind= inner ( amazon_kinesis_stream_CL | summarize TimeGenerated=max(TimeGenerated) by amazon_kinesis_stream_name_s) on TimeGenerated, amazon_kinesis_stream_name_s | project amazon_kinesis_stream_name_s, retention_period_d ",
                                            "displayName": "Kinesis Retention Period"
                                        },
                                        {
                                            "query": "amazon_kinesis_stream_CL  | join kind= inner ( amazon_kinesis_stream_CL | summarize TimeGenerated=max(TimeGenerated) by amazon_kinesis_stream_name_s) on TimeGenerated, amazon_kinesis_stream_name_s | project amazon_kinesis_stream_name_s, encryption_type_s  ",
                                            "displayName": "Kinesis Encryption"
                                        },
                                        {
                                            "query": "amazon_kinesis_shard_CL | join kind= inner( amazon_kinesis_shard_CL | summarize TimeGenerated=max(TimeGenerated) by amazon_kinesis_shard_name_s ) on TimeGenerated, amazon_kinesis_shard_name_s | project stream_name_s, amazon_kinesis_shard_name_s",
                                            "displayName": "Kinesis Shards"
                                        }
                                    ]
                                }
                            }
                        ],
                        "Filters": [
                            {
                                "FieldName": "amazon_kinesis_stream_name_s",
                                "QueryForValues": "amazon_kinesis_stream_CL | distinct amazon_kinesis_stream_name_s",
                                "Title": "Stream"
                            }
                        ],
                        "OverviewTile": {
                            "Id": "LineChartCalloutBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 2,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "amazon_kinesis_stream_CL | summarize sum(incoming_records_d) by amazon_kinesis_stream_name_s",
                                    "Callout": {
                                        "Title": "Records Inserted",
                                        "Series": "",
                                        "Operation": "Sum"
                                    },
                                    "yAxis": {
                                        "isLogarithmic": false,
                                        "units": {
                                            "baseUnitType": "",
                                            "baseUnit": "",
                                            "displayUnit": ""
                                        },
                                        "customLabel": "Records"
                                    }
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": false,
                                        "Query": "search * | limit 1 | project TimeGenerated",
                                        "Message": ""
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}