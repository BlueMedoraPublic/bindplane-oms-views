{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Amazon SQS Environment",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Amazon SQS Environment')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Amazon SQS Environment",
                        "Name": "Amazon SQS Environment",
                        "Author": "service.azure@bluemedora.com",
                        "Source": "Local",
                        "Version": 2,
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "SQS Overview",
                                        "NewGroup": false,
                                        "Color": "#dd5900"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Amazon SQS",
                                        "Link": {
                                            "Label": "More info",
                                            "Url": "https://docs.bindplane.bluemedora.com/docs/sqs"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "Overview",
                                            "Content": "Amazon SQS is a fully managed message queuing service that makes it easy to manage and scale microservices and serverless applications."
                                        },
                                        {
                                            "Title": "Key Performance Indicators",
                                            "Content": "In Amazon SQS, it is important to monitor the throughput of a queue. \n\nIf the queue is growing faster than it is being consumed, then the application will not be able to process messages in a timely manner."
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "SQS Queue Operations",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Messages Received",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_sqs_queue_CL  | where ${Queue} | summarize avg(iif(isempty(number_of_messages_received_d),0.0,number_of_messages_received_d)) by amazon_sqs_queue_name_s | order by avg_ desc",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Messages"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Messages Sent",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "amazon_sqs_queue_CL | where ${Queue} | summarize avg(iif(isempty(number_of_messages_sent_d),0.0,number_of_messages_sent_d)) by amazon_sqs_queue_name_s | order by avg_ desc ",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Messages"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Messages Deleted",
                                                "Subtitle": " "
                                            },
                                            "LineChart": {
                                                "Query": "amazon_sqs_queue_CL | where ${Queue} | summarize avg(iif(isempty(number_of_messages_deleted_d),0.0,number_of_messages_deleted_d)) by amazon_sqs_queue_name_s | order by avg_ desc",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": "Messages"
                                                },
                                                "NavigationSelect": {}
                                            }
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartCalloutBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "SQS Queue Delay",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Messages Delayed",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_sqs_queue_CL | where ${Queue} | summarize avg(iif(isempty(approximate_number_of_messages_delayed_d),0.0,approximate_number_of_messages_delayed_d)) by amazon_sqs_queue_name_s | order by avg_ desc",
                                        "Callout": {
                                            "Title": "Average Delayed",
                                            "Series": "",
                                            "Operation": "Average"
                                        },
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": "Messages"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_sqs_queue_CL | where ${Queue} | summarize avg(iif(isempty(approximate_number_of_messages_delayed_d),0.0,approximate_number_of_messages_delayed_d)) by amazon_sqs_queue_name_s | order by avg_ desc",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Sum",
                                        "ColumnsTitle": {
                                            "Name": "Queue",
                                            "Value": "Messages Delayed"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "LineChartCalloutBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "SQS Queue Age",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Oldest Message Age",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "amazon_sqs_queue_CL | where ${Queue} | summarize avg(iif(isempty(approximate_age_of_oldest_message_d),0.0,approximate_age_of_oldest_message_d)) by amazon_sqs_queue_name_s | order by avg_ desc ",
                                        "Callout": {
                                            "Title": "Avg Age",
                                            "Series": "",
                                            "Operation": "Average"
                                        },
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "TimeRange",
                                                "baseUnit": "Seconds",
                                                "displayUnit": "Seconds"
                                            },
                                            "customLabel": "Age"
                                        },
                                        "NavigationSelect": {}
                                    },
                                    "List": {
                                        "Query": "amazon_sqs_queue_CL | where ${Queue}  | summarize avg(coalesce(approximate_age_of_oldest_message_d,0.0)) by amazon_sqs_queue_name_s | order by avg_ desc",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Last Sample",
                                        "ColumnsTitle": {
                                            "Name": "Queue",
                                            "Value": "Age"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "search {selected item} | sort by TimeGenerated desc",
                                        "NavigationSelect": {
                                            "NavigationQuery": "search {selected item} | sort by TimeGenerated desc"
                                        }
                                    }
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Additional Info",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "amazon_sqs_queue_CL  | distinct amazon_sqs_queue_name_s, maximum_message_size_d",
                                            "displayName": "Message Maximum Size"
                                        },
                                        {
                                            "query": "amazon_sqs_queue_CL  | distinct amazon_sqs_queue_name_s, visibility_timeout_d",
                                            "displayName": "Message Visibility Timeout"
                                        },
                                        {
                                            "query": "amazon_sqs_queue_CL  | distinct amazon_sqs_queue_name_s, receive_message_wait_time_d",
                                            "displayName": "Message Receive Wait Time"
                                        },
                                        {
                                            "query": "amazon_sqs_queue_CL  | distinct amazon_sqs_queue_name_s, message_retention_period_d",
                                            "displayName": "Message Retention Period"
                                        },
                                        {
                                            "query": "amazon_sqs_queue_CL  | summarize avg(approximate_number_of_messages_visible_d), avg(approximate_number_of_messages_not_visible_d) by amazon_sqs_queue_name_s",
                                            "displayName": "Message Visibility Summary"
                                        }
                                    ]
                                }
                            }
                        ],
                        "Filters": [
                            {
                                "FieldName": "amazon_sqs_queue_name_s",
                                "QueryForValues": "amazon_sqs_queue_CL | distinct amazon_sqs_queue_name_s",
                                "Title": "Queue"
                            }
                        ],
                        "OverviewTile": {
                            "Id": "LineChartCalloutBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 2,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "amazon_sqs_queue_CL | summarize avg(approximate_number_of_messages_d) by amazon_sqs_queue_name_s",
                                    "Callout": {
                                        "Title": "Approximate Messages",
                                        "Series": "",
                                        "Operation": "Last Sample"
                                    },
                                    "yAxis": {
                                        "isLogarithmic": false,
                                        "units": {
                                            "baseUnitType": "",
                                            "baseUnit": "",
                                            "displayUnit": ""
                                        },
                                        "customLabel": "Messages"
                                    }
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": false,
                                        "Query": "search * | limit 1 | project TimeGenerated",
                                        "Message": ""
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}